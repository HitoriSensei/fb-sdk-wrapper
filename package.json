{
  "name": "fb-sdk-wrapper",
  "version": "0.3.0",
  "description": "Wraps around the Facebook Javascript SDK to make it usable as a ES6 module",
  "main": "./lib/index.js",
  "repository": "https://github.com/erikhagreis/fb-sdk-wrapper",
  "author": "Erik Hagreis",
  "keywords": [
    "facebook",
    "graph",
    "sdk",
    "es6",
    "promise"
  ],
  "license": "MIT",
  "private": false,
  "scripts": {
    "build": "babel src --out-dir lib --ignore spec.js",
    "prepare": "npm run build",
    "test": "jest"
  },
  "devDependencies": {
    "@babel/cli": "^7.5.0",
    "@babel/core": "^7.5.4",
    "@babel/preset-env": "^7.5.4",
    "husky": "^3.0.0",
    "jest": "^24.8.0",
    "lint-staged": "^9.2.0",
    "prettier": "^1.18.2"
  },
  "lint-staged": {
    "src/**/*.{js,jsx,json,css}": [
      "prettier --write",
      "git add"
    ]
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged",
      "pre-push": "npm run test"
    }
  }
}
